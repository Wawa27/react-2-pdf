{
  "name": "react2pdf",
  "version": "0.3.0",
  "type": "module",
  "main": "./build/React2Pdf.js",
  "license": "MIT",
  "repository": "https://github.com/Wawa27/react2pdf.git",
  "homepage": "https://github.com/Wawa27/react2pdf#readme",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm src/react.tsx",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "example:hello_world": "node --experimental-specifier-resolution=node --loader ts-node/esm examples/render.tsx"
  },
  "keywords": [
    "pdf",
    "react",
    "html",
    "css"
  ],
  "dependencies": {
    "puppeteer": "^10.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.4.12",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
